    <DataSet Name="Routings">
      <Query>
        <DataSourceName>BDOUTPUT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CFGID">
            <Value>=Parameters!DetailID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @CFGID VARCHAR(10) = 'MULL FUN'
;WITH    GROUP1
          AS ( SELECT  [O].[ComponentID] ,
        [O].[OperationID] ,
        [O].[OperationNo] ,
        [OA].[Name] ,
        [OA].[Value] ,
        BC.ComponentName ,
        ISNULL(CMP7.Value,CMP6.Value) [Component Description]
		
FROM    [dbo].[BDCConfigurations] CF
        INNER JOIN [dbo].[BDCBOMComponents] BC ON [BC].[CfgID] = [CF].[CfgID]
        RIGHT JOIN [dbo].[BDCOperations] O ON [BC].[ComponentID] = [O].[ComponentID]
        INNER JOIN [dbo].[BDCOperationAttributes] OA ON [O].[OperationID] = [OA].[OperationID]
        LEFT JOIN ( SELECT  [BDCBOMComponentAttributes].[ComponentID] AS ComponentID ,
                            [BDCComponentAttributes].[Value]
                    FROM    [dbo].[BDCComponentAttributes]
                            INNER JOIN [dbo].[BDCComponents] ON [BDCComponents].[ComponentID] = [BDCComponentAttributes].[ComponentID]
                            INNER JOIN [dbo].[BDCConfigurations] ON [BDCConfigurations].[CfgID] = [BDCComponents].[CfgID]
                            INNER JOIN [dbo].[BDCBOMComponents] ON [BDCBOMComponents].[CfgID] = [BDCConfigurations].[CfgID]
                            INNER JOIN [dbo].[BDCBOMComponentAttributes] ON [BDCBOMComponentAttributes].[ComponentID] = [BDCBOMComponents].[ComponentID]
                    WHERE   [BDCBOMComponentAttributes].[Value] = [ComponentSequence]
                            AND [BDCBOMComponentAttributes].[Name] = 'COMP_SEQ'
                            AND ComponentAttributeName = 'O-COMP_DESC'
                            AND [BDCComponentAttributes].[Value] IS NOT NULL
                            AND [BDCComponentAttributes].[Value] != ' '
                  ) AS CMP7 ON [O].[ComponentID] = [CMP7].[ComponentID]
        LEFT JOIN ( SELECT DISTINCT
                            [CA].[ComponentAttributeName] ,
                            [CA].[Value] ,
                            CM.ComponentID,
							CM.CfgID
                    FROM    [dbo].[BDCComponentAttributes] CA
                            INNER JOIN [dbo].[BDCComponents] CM ON [CM].[ComponentID] = [CA].[ComponentID]
                            INNER JOIN [dbo].[BDCConfigurations] CF ON [CF].[CfgID] = [CM].[CfgID]
                           
                    WHERE   [CA].[ComponentAttributeName] = 'ConfigurationCode' 
                            AND [CA].[Value] IS NOT NULL
                            AND [CM].[ComponentSequence] = '1'
                  ) CMP6 ON CMP6.CfgID = CF.CfgID
WHERE   [CF].[ConfigurationID] = @CFGID
        AND [OA].[Name] IN ( 'Operation Name', 'Operation Code', 'Requirement',
                             'Costing Resource', 'Run Time', 'Setup Time',
                             'Queue Time Before' )
        AND [OA].[Value] != ''
             ),
        GROUP2
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Requirement'
             ),
        GROUP3
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Operation Code'
             ),
        GROUP4
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Operation Name'
             ),
        GROUP5
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Costing Resource'
             ),
        GROUP6
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Run Time'
             ),
        GROUP7
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Setup Time'
             ),
        GROUP8
          AS ( SELECT   *
               FROM     GROUP1
               WHERE    [GROUP1].[Name] = 'Queue Time Before'
             )
    SELECT DISTINCT
            [GROUP1].[ComponentID]
           ,[GROUP1].[OperationID]
           ,[GROUP1].[OperationNo]
		   ,GROUP1.ComponentName
		   ,GROUP1.[Component Description]
           ,[GROUP2].[Value] AS REQ
           ,[GROUP3].[Value] AS OPCODE
           ,[GROUP4].[Value] AS OPNAME
           ,[GROUP5].[Value] AS CSTRES
           ,[GROUP6].[Value] AS RTME
           ,[GROUP7].[Value] AS STME
           ,[GROUP8].[Value] AS QTME
    FROM    GROUP1
            LEFT JOIN GROUP2 ON [GROUP1].[OperationID] = [GROUP2].[OperationID]
            LEFT JOIN GROUP3 ON [GROUP1].[OperationID] = [GROUP3].[OperationID]
            LEFT JOIN GROUP4 ON [GROUP1].[OperationID] = [GROUP4].[OperationID]
            LEFT JOIN GROUP5 ON [GROUP1].[OperationID] = [GROUP5].[OperationID]
            LEFT JOIN GROUP6 ON [GROUP1].[OperationID] = [GROUP6].[OperationID]
            LEFT JOIN GROUP7 ON [GROUP1].[OperationID] = [GROUP7].[OperationID]
            LEFT JOIN GROUP8 ON [GROUP1].[OperationID] = [GROUP8].[OperationID]
    ORDER BY [GROUP1].[ComponentID]
           ,[GROUP1].[OperationNo]</CommandText>
      </Query>
