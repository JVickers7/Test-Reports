  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BDOUTPUT</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HeaderID">
            <Value>=Parameters!HeaderID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DetailID">
            <Value>=Parameters!DetailID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @HeaderID VARCHAR(10) = 'Simulator' ,
--    @DetailID VARCHAR(10) = 'MULL FUN';
BEGIN
    SET NOCOUNT ON;
    WITH    BOMReports ( ParentComponentID, ComponentID, CfgID, MaterialID, level, iscomponent, CONSUMEDOPERATIONID )
              AS ( SELECT   [PBC].[ParentComponentID] ,
                            [PBC].[ComponentID] ,
                            [PBC].[CfgID] ,
                            [PBC].[ComponentName] ,
                            0 AS level ,
                            [PBC].[IsComponent] ,
                            [PBC].[ConsumedOperationID]
                   FROM     [dbo].[BDCBOMComponents] AS PBC
                   WHERE    [PBC].[ParentComponentID] IS NULL
                            AND [PBC].[CfgID] = ( SELECT    CfgID
                                                  FROM      [dbo].[BDCConfigurations]
                                                  WHERE     HeaderID = @HeaderID
                                                            AND ConfigurationID = @DetailID
                                                )
                   UNION ALL
                   SELECT   [PBC].[ParentComponentID] ,
                            [PBC].[ComponentID] ,
                            [PBC].[CfgID] ,
                            [PBC].[ComponentName] ,
                            0 AS level ,
                            [PBC].[IsComponent] ,
                            [PBC].[ConsumedOperationID]
                   FROM     [dbo].[BDCBOMComponents] AS PBC
                            INNER JOIN [dbo].[BDCBOMComponents] AS PBP ON [PBC].[ParentComponentID] = [PBP].[ComponentID]
                   WHERE    [PBC].[CfgID] = ( SELECT    CfgID
                                              FROM      [dbo].[BDCConfigurations]
                                              WHERE     HeaderID = @HeaderID
                                                        AND ConfigurationID = @DetailID
                                            )
                 ),
            BOM_TEMP
              AS ( SELECT DISTINCT
                            [BDCBOMComponentAttributes].[ComponentID] ,
                            Value ,
                            Name
                   FROM     [dbo].[BDCBOMComponentAttributes] ,
                            BOMReports
                   WHERE    [BOMReports].[ComponentID] = [BDCBOMComponentAttributes].[ComponentID]
                            AND Name IN ( 'Part Number', 'UOM', 'Description',
                                          'Quantity', 'Variable Scrap',
                                          'Flushing Principle', 'COMP_SEQ',
                                          'AXMaterial Type' )
                 ),
            CA_TEMP
              AS ( SELECT DISTINCT
                            [CA].[ComponentAttributeName] ,
                            [CA].[Value] ,
                            [BOM_TEMP].[ComponentID]
                   FROM     [dbo].[BDCComponentAttributes] CA
                            INNER JOIN [dbo].[BDCComponents] CM ON [CM].[ComponentID] = [CA].[ComponentID]
                            INNER JOIN [dbo].[BDCConfigurations] CF ON [CF].[CfgID] = [CM].[CfgID]
                            INNER JOIN BOMReports ON [CF].[CfgID] = [BOMReports].[CfgID]
                            INNER JOIN BOM_TEMP ON [BOM_TEMP].[ComponentID] = [BOMReports].[ComponentID]
                   WHERE    [CA].[ComponentAttributeName] IN (
                            'O-PLANT_LOCATION', 'DATETIME',
                            'ConfigurationCode', 'DESC1', 'DESC2' )
                            AND [CA].[Value] IS NOT NULL
                            AND [CM].[ComponentSequence] = '1'
                 )
        SELECT  REPLICATE('.', [BR].[level] * 3) AS Indent ,
                ROW_NUMBER() OVER ( ORDER BY CASE WHEN [BR].[ParentComponentID] IS NULL
                                                  THEN 0
                                                  WHEN [BR].[iscomponent] = 0
                                                       AND [BR].[level] = 1
                                                  THEN 1
                                                  WHEN [BR].[iscomponent] = 0
                                                       AND [BR].[level] = 2
                                                  THEN 2
                                                  WHEN [BR].[iscomponent] = 0
                                                       AND [BR].[level] = 3
                                                  THEN 3
                                                  WHEN [BR].[iscomponent] = 1
                                                       AND [BR].[level] = 1
                                                  THEN 2
                                                  WHEN [BR].[iscomponent] = 1
                                                       AND [BR].[level] = 2
                                                  THEN 3
                                                  WHEN [BR].[iscomponent] = 1
                                                       AND [BR].[level] = 3
                                                  THEN 4
                                                  ELSE 5
                                             END, [BR].[ComponentID] ) AS Row ,
                [BR].[ComponentID] ,
                [BR].[ParentComponentID] ,
                [BR].[CfgID] ,
                [BR].[MaterialID] ,
                [BBCA].[Value] AS PartNumber ,
                [E].[DRAWINGNO] AS DWGNO ,
                [BBCA2].[Value] AS UOM ,
                --CASE WHEN [CMP7].[Value] IS NULL THEN [BBCA3].[Value]
                --     ELSE ( [BBCA3].[Value] + ' - ' + [CMP7].[Value] )
                --END AS RuleDescription ,
                CASE WHEN BBCA7.Value IS NULL
                     THEN [BBCA3].[Value]
                          + CASE WHEN [CMP7].[Value] IS NULL THEN ' '
                                 ELSE ( ' - ' + [CMP7].[Value] )
                            END
                     ELSE [BBCA3].[Value]
                END AS RuleDescription ,
                [BBCA4].[VALUE_ROUNDED] AS Quantity ,
                [BBCA7].[Value] AS FlushingMeth ,
                [CMP6].[Value] AS VariantDesc ,
                [OPNO].[OperationNo] ,
                [CMP8].[Value] AS DTETME ,
                [CMP9].[Value] AS PLANT ,
                [EPT].[NAME] AS AXDESCR1 ,
                @DetailID AS SIMCFGID ,
                ISNULL([BBCA8].[SCRAP_RND], 0) AS SCRAP_PCT ,
                CAST(( ( [BBCA4].[VALUE_ROUNDED]
                         * ( ISNULL([BBCA8].[SCRAP_RND], 0) * 0.01 ) )
                       + [BBCA4].[VALUE_ROUNDED] ) AS DECIMAL(20, 3)) AS TOTAL_QTY ,
                [BBCA2].[Value] ,
                CASE WHEN ( SELECT  COUNT(*)
                            FROM    ( SELECT    [EP].[DISPLAYPRODUCTNUMBER] ,
                                                [U].[SYMBOL]
                                      FROM      [LCI-BI-100].[LCIAXREP].[dbo].[UNITOFMEASURECONVERSION] UOM
                                                INNER JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESPRODUCT] EP ON [EP].[RECID] = UOM.[PRODUCT]
                                                INNER JOIN [LCI-BI-100].[LCIAXREP].[dbo].[UNITOFMEASURE] U ON [U].[RECID] = [UOM].[FROMUNITOFMEASURE]
                                      UNION ALL
                                      SELECT    [IUOM].[ITEMID] ,
                                                [IUOM].[UNITID]
                                      FROM      [LCI-BI-100].[LCIAXREP].[dbo].[INVENTTABLEMODULE]
                                                AS IUOM
                                    ) A
                            WHERE   [A].[DISPLAYPRODUCTNUMBER] = [BBCA].[Value]
                                    AND [A].[SYMBOL] = [BBCA2].[Value]
                          ) = 0 THEN 'BADUOM'
                     ELSE 'GOODUOM'
                END AS NAVUOMCHECK ,
                [BR].[iscomponent] ,
                ETDG.NAME AS [Tracking Dimension] ,
                BBCA9.[Value] [AX Material Type]
        FROM    BOMReports AS BR
                LEFT JOIN ( SELECT  *
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'Part Number'
                          ) AS BBCA ON [BR].[ComponentID] = [BBCA].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'UOM'
                          ) AS BBCA2 ON [BR].[ComponentID] = [BBCA2].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'Description'
                          ) AS BBCA3 ON [BR].[ComponentID] = [BBCA3].[ComponentID]
                LEFT JOIN ( SELECT  * ,
                                    CAST(( ROUND([BOM_TEMP].[Value], 3) ) AS DECIMAL(20,
                                                              3)) AS VALUE_ROUNDED
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'Quantity'
                          ) AS BBCA4 ON [BR].[ComponentID] = [BBCA4].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'Flushing Principle'
                          ) AS BBCA7 ON [BR].[ComponentID] = [BBCA7].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'AXMaterial Type'
                          ) AS BBCA9 ON [BR].[ComponentID] = [BBCA9].[ComponentID]
                LEFT JOIN ( SELECT  * ,
                                    CAST(( ROUND(CAST([BOM_TEMP].[Value] AS FLOAT),
                                                 3) ) AS DECIMAL(20, 3)) AS SCRAP_RND
                            FROM    BOM_TEMP
                            WHERE   [BOM_TEMP].[Name] = 'Variable Scrap'
                          ) AS BBCA8 ON [BR].[ComponentID] = [BBCA8].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    CA_TEMP
                            WHERE   [CA_TEMP].[ComponentAttributeName] = 'ConfigurationCode'
                          ) AS CMP6 ON [BR].[ComponentID] = [CMP6].[ComponentID]
                LEFT JOIN ( SELECT  [BDCBOMComponentAttributes].[ComponentID] AS ComponentID ,
                                    [BDCComponentAttributes].[Value]
                            FROM    [dbo].[BDCComponentAttributes]
                                    INNER JOIN [dbo].[BDCComponents] ON [BDCComponents].[ComponentID] = [BDCComponentAttributes].[ComponentID]
                                    INNER JOIN [dbo].[BDCConfigurations] ON [BDCConfigurations].[CfgID] = [BDCComponents].[CfgID]
                                    INNER JOIN [dbo].[BDCBOMComponents] ON [BDCBOMComponents].[CfgID] = [BDCConfigurations].[CfgID]
                                    INNER JOIN [dbo].[BDCBOMComponentAttributes] ON [BDCBOMComponentAttributes].[ComponentID] = [BDCBOMComponents].[ComponentID]
                            WHERE   [BDCBOMComponentAttributes].[Value] = [ComponentSequence]
                                    AND [BDCBOMComponentAttributes].[Name] = 'COMP_SEQ'
                                    AND ComponentAttributeName = 'O-COMP_DESC'
                                    AND [BDCComponentAttributes].[Value] IS NOT NULL
                                    AND [BDCComponentAttributes].[Value] != ' '
                          ) AS CMP7 ON [BR].[ComponentID] = [CMP7].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    CA_TEMP
                            WHERE   [CA_TEMP].[ComponentAttributeName] = 'DATETIME'
                          ) AS CMP8 ON [BR].[ComponentID] = [CMP8].[ComponentID]
                LEFT JOIN ( SELECT  *
                            FROM    CA_TEMP
                            WHERE   [CA_TEMP].[ComponentAttributeName] = 'O-PLANT_LOCATION'
                          ) AS CMP9 ON [BR].[ComponentID] = [CMP9].[ComponentID]
                LEFT JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESPRODUCT] AS E ON [BBCA].[Value] = [E].[DISPLAYPRODUCTNUMBER]
                LEFT JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESPRODUCTTRANSLATION] EPT ON [E].[RECID] = [EPT].[PRODUCT]
                LEFT JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESTRACKINGDIMENSIONGROUPITEM] ETGI ON ETGI.ITEMID = E.DISPLAYPRODUCTNUMBER
                LEFT JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESTRACKINGDIMENSIONGROUP] ETDG ON ETGI.TRACKINGDIMENSIONGROUP = ETDG.RECID
                LEFT JOIN ( SELECT  *
                            FROM    [dbo].[BDCOperations]
                          ) AS OPNO ON [OPNO].[OperationID] = [BR].[CONSUMEDOPERATIONID]
        ORDER BY Row;
END;

</CommandText>
      </Query>
      <Fields>
        <Field Name="Indent">
          <DataField>Indent</DataField>
          <rd:TypeName>System.String</rd:TypeName>
