    <DataSource Name="BDOUTPUT">
      <DataSourceReference>BDC70Output</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>dbcb2a3b-8eb9-4c75-a342-5f6ea52f88f4</rd:DataSourceID>
