      <Query>
        <DataSourceName>BDMODEL7DEV</DataSourceName>
        <CommandText>WITH CFGS AS
(SELECT A.HeaderID, A.ConfigurationID AS VARIANT, A.CfgID FROM OPENQUERY ([LCI-SQL-FS7],
 'SELECT CAST(InProgressInputXML as VARCHAR(max)) AS IPIX, CAST(InProgressOutputXML as VARCHAR(max)) AS IPOX,
CAST(InputXML as VARCHAR(max)) AS IX, CAST(OutputXML as VARCHAR(max)) AS OX, HeaderID, ConfigurationID, CfgID
FROM [LCINAVREP].dbo.[BDCConfigurations]') A ),

CMPS AS
(SELECT HeaderID, VARIANT, CFGS.CfgID, ComponentID, RulesetName as PN, [Description] AS VD1,[Description 2] as VD2
FROM [LCI-SQL-FS7].[LCINAVREP].dbo.[BDCComponents], CFGS
LEFT JOIN [LCI-SQL-FS7].[LCINAVREP].dbo.[LCI$Item Variant] ON VARIANT COLLATE SQL_Latin1_General_CP1_CI_AS = Code
WHERE  BDCComponents.CfgID = CFGS.CfgID AND HeaderID = 'VARIANT' AND ComponentSequence = '1' AND Blocked = '0'),

HRARCHY AS
(SELECT Name, RG.RulesetGroupID FROM [LCI-SQL-FS2].[BDMODEL7DEV].[dbo].[RulesetGroups] RG
WHERE RG.Name = @PARENTRULESET
UNION ALL
SELECT RG.Name, RG.RulesetGroupID FROM [LCI-SQL-FS2].[BDMODEL7DEV].[dbo].[RulesetGroups] RG
JOIN HRARCHY H ON H.RulesetGroupID = RG.ParentRulesetGroupID),

PARTNAMES AS
(SELECT X.* FROM HRARCHY X WHERE Name NOT LIKE '*%'),

VARIANTS AS 
(SELECT * FROM CMPS, PARTNAMES WHERE PN = Name),

VARIABLEVALUES AS
(SELECT PN, HeaderID, VARIANT, CA.[ComponentID], CA.[ComponentAttributeName], CA.[Value], VD1, VD2
FROM [LCI-SQL-FS7].[LCINAVREP].dbo.[BDCComponentAttributes] CA, VARIANTS
WHERE VARIANTS.ComponentID = CA.ComponentID AND CA.[ComponentAttributeName] IN (@CALIST))

SELECT * FROM VARIABLEVALUES ORDER BY VARIANT
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@PARENTRULESET">
            <Value>=Parameters!PARENTRULESET.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CALIST">
            <Value>=Parameters!CALIST.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
