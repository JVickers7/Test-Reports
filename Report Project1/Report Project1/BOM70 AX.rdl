  <DataSets>
    <DataSet Name="BDC70Output">
      <Query>
        <DataSourceName>BDC70Output</DataSourceName>
        <CommandText>DECLARE @HeaderID AS nvarchar(50);
DECLARE @DetailID AS nvarchar(50);

--SAMPLE DATA FOR TESTING
--SET @HeaderID = 'Simulator'
--SET @DetailID = 'ReportSpecTest'

BEGIN
SET NOCOUNT ON;
WITH BOMReports (ParentComponentID, ComponentID, CfgID, MaterialID, level, iscomponent, CONSUMEDOPERATIONID) AS 
(SELECT PBC.ParentComponentID, PBC.ComponentID, PBC.CfgID, PBC.ComponentName, 0 AS level, PBC.iscomponent, PBC.CONSUMEDOPERATIONID
FROM BDCBOMComponents AS PBC
WHERE ParentComponentID IS NULL
AND PBC.CfgID = (SELECT CfgID FROM BDCConfigurations WHERE HeaderID = @HeaderID and ConfigurationID = @DetailID)
UNION ALL
SELECT PBC.ParentComponentID, PBC.ComponentID, PBC.CfgID, PBC.ComponentName, 0 as level,PBC.iscomponent, PBC.CONSUMEDOPERATIONID
FROM BDCBOMComponents AS PBC
INNER JOIN BDCBOMComponents AS PBP ON PBC.ParentComponentID = PBP.ComponentID
WHERE PBC.CfgID = (SELECT CfgID FROM BDCConfigurations WHERE HeaderID = @HeaderID and ConfigurationID = @DetailID)),

BOM_TEMP AS (SELECT DISTINCT BDCBOMComponentAttributes.ComponentID , Value, Name
FROM BDCBOMComponentAttributes, BOMReports 
WHERE BOMReports.ComponentID = BDCBOMComponentAttributes.ComponentID 
AND Name IN ('Part Number','UOM','Description','Quantity', 'Variable Scrap','Flushing Principle','COMP_SEQ')),

CA_TEMP AS ( SELECT DISTINCT ComponentAttributeName, CA.Value, BOM_TEMP.ComponentID 
FROM  [BDCComponentAttributes] CA 
INNER JOIN BDCComponents CM ON CM.ComponentID = CA.ComponentID
INNER JOIN BDCConfigurations CF ON CF.CfgID = CM.CfgID
INNER JOIN BOMReports ON CF.CfgID = BOMReports.CfgID
INNER JOIN BOM_TEMP ON BOM_TEMP.ComponentID = BOMReports.ComponentID
WHERE ComponentAttributeName in ('O-PLANT_LOCATION' ,'DATETIME','ConfigurationCode', 'DESC1','DESC2')
AND CA.Value IS NOT NULL and ComponentSequence = '1' )

SELECT REPLICATE( '.', level * 3 ) as Indent, 
ROW_NUMBER() OVER (ORDER BY
case 
when Parentcomponentid is null then 0 
when iscomponent = 0 and level = 1 then 1
when iscomponent = 0 and level = 2 then 2
when iscomponent = 0 and level = 3 then 3
when iscomponent = 1 and level = 1 then 2
when iscomponent = 1 and level = 2 then 3
when iscomponent = 1 and level = 3 then 4 
else 5
END, br.componentid) AS Row,
BR.ComponentID, 
ParentComponentID, 
CfgID, 
MaterialID,
BBCA.Value AS PartNumber, 
E.[DRAWINGNO] AS DWGNO,
BBCA2.Value AS UOM, 
CASE WHEN CMP7.VALUE IS NULL THEN BBCA3.Value ELSE (BBCA3.Value + ' - ' + CMP7.VALUE) END AS RuleDescription, 
BBCA4.VALUE_ROUNDED AS Quantity,
BBCA7.Value AS FlushingMeth,
CMP6.Value As VariantDesc,
OPNO.OperationNo,
CMP8.VALUE AS DTETME,
CMP9.Value AS PLANT,
E.[SEARCHNAME] AS AXDESCR1,
@DetailID AS SIMCFGID,
ISNULL(SCRAP_RND,0) AS SCRAP_PCT,
CAST(((BBCA4.VALUE_ROUNDED * (ISNULL(SCRAP_RND,0) * 0.01))+ BBCA4.VALUE_ROUNDED) AS DECIMAL (20,3)) AS TOTAL_QTY,
bbca2.[Value],
CASE WHEN ( SELECT  COUNT(*)
    FROM    [LCI-BI-100].[LCIAXREP].[dbo].[INVENTTABLEMODULE] 
            AS IUOM
    WHERE   BBCA.Value= IUOM.[ITEMID]
            AND BBCA2.Value = IUOM.[UNITID]
  ) = 0 THEN 'BADUOM'
ELSE 'GOODUOM'
END AS NAVUOMCHECK,
iscomponent
FROM BOMReports AS BR 

LEFT JOIN (SELECT * FROM BOM_TEMP WHERE Name = 'Part Number') AS BBCA ON BR.ComponentID = BBCA.ComponentID
LEFT JOIN (SELECT * FROM BOM_TEMP WHERE Name = 'UOM') AS BBCA2 ON BR.ComponentID = BBCA2.ComponentID 
LEFT JOIN (SELECT * FROM BOM_TEMP WHERE Name = 'Description') AS BBCA3 ON BR.ComponentID = BBCA3.ComponentID 
LEFT JOIN (SELECT *, CAST((ROUND(Value,3)) AS DECIMAL (20,3)) AS VALUE_ROUNDED FROM BOM_TEMP WHERE Name = 'Quantity') AS BBCA4 ON BR.ComponentID = BBCA4.ComponentID
LEFT JOIN (SELECT * FROM BOM_TEMP WHERE Name = 'Flushing Principle') AS BBCA7 ON BR.ComponentID = BBCA7.ComponentID
LEFT JOIN (SELECT *, CAST((ROUND(CAST(Value AS FLOAT),3)) AS DECIMAL (20,3)) AS SCRAP_RND FROM BOM_TEMP WHERE Name = 'Variable Scrap') AS BBCA8
ON BR.ComponentID = BBCA8.ComponentID
LEFT JOIN (SELECT * FROM CA_TEMP WHERE ComponentAttributeName = 'ConfigurationCode') AS CMP6 ON BR.ComponentID = CMP6.ComponentID
LEFT JOIN (SELECT BDCBOMComponentAttributes.ComponentID AS ComponentID, BDCComponentAttributes.Value
FROM [BDCComponentAttributes]
INNER JOIN BDCComponents ON BDCComponents.ComponentID = BDCComponentAttributes.ComponentID
INNER JOIN Bdcconfigurations ON BDCConfigurations.CfgID = BDCComponents.CfgID
INNER JOIN BDCBOMComponents ON BDCBOMComponents.CfgID = BDCConfigurations.CfgID
INNER JOIN BDCBOMComponentAttributes ON BDCBOMComponentAttributes.ComponentID = BDCBOMComponents.ComponentID 
WHERE [BDCBOMComponentAttributes].VALUE = [BDCComponents].[ComponentSequence]
AND BDCBOMCOMPONENTATTRIBUTES.NAME = 'COMP_SEQ' AND ComponentAttributeName = 'O-COMP_DESC'
and BDCComponentAttributes.Value is not null and BDCComponentAttributes.Value != ' ') AS CMP7
ON  BR.ComponentID = CMP7.ComponentID
LEFT JOIN (SELECT * FROM CA_TEMP WHERE ComponentAttributeName = 'DATETIME') AS CMP8 ON BR.ComponentID = CMP8.ComponentID
LEFT JOIN (SELECT * FROM CA_TEMP WHERE ComponentAttributeName = 'O-PLANT_LOCATION') AS CMP9 ON BR.ComponentID = CMP9.ComponentID
LEFT JOIN [LCI-BI-100].[LCIAXREP].[dbo].[ECORESPRODUCT] AS E ON BBCA.Value = E.[DISPLAYPRODUCTNUMBER]


LEFT JOIN (SELECT * FROM [BDCOperations]) AS OPNO ON OPNO.OperationID = BR.ConsumedOperationID

order by Row
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
