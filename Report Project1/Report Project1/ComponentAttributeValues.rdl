  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BUYDESIGNREP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SALES_ORDER_NUMBER">
            <Value>=Parameters!SALES_ORDER_NUMBER.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SALES_ORDER_LINE">
            <Value>=Parameters!SALES_ORDER_LINE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @SALES_ORDER_NUMBER VARCHAR(20) = 'RPO-203-188733'
--   ,@SALES_ORDER_LINE VARCHAR(20) = '10000'
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
SET NOCOUNT ON;
        
WITH    GROUP1
          AS ( SELECT DISTINCT
                        CASE WHEN [CFG].[HeaderID] LIKE '%RRPO%'
                             THEN SUBSTRING([CFG].[HeaderID],
                                            32, 14)
                             ELSE SUBSTRING([CFG].[HeaderID],
                                            32, 13)
                        END AS SALES_ORDER_NUM
                       ,[CFG].[ConfigurationID] AS LINE_NUM
                       ,[CA].[ComponentAttributeName]
                       ,[CA].[Value]
                       ,[SO].[ValueDescription]
                       ,[SO].[OptionSequence]
                       ,CASE WHEN [SO].[ScreenID] IS NULL
                             THEN '9999'
                             ELSE RIGHT([SO].[ScreenID], 3)
                        END AS SID
                       ,CASE WHEN [CLS].[ClassificationID] IS NOT NULL
                             THEN 'TRUE'
                             ELSE 'FALSE'
                        END AS CLS
               FROM     [dbo].[BDCConfigurations] CFG
                       ,[dbo].[BDCComponents] CMP
                       ,[dbo].[BDCComponentAttributes] CA
                        LEFT JOIN [dbo].[BDCScreenOptions] SO ON [SO].[ComponentAttributeID] = [CA].[ComponentAttributeID]
                        LEFT JOIN [dbo].[BDCComponentAttributeClassifications] CLS ON [CLS].[ComponentAttributeID] = [CA].[ComponentAttributeID]
               WHERE    [CFG].[CfgID] = [CMP].[CfgID]
                        AND [CMP].[ComponentID] = [CA].[ComponentID]
                        AND [CMP].[ComponentSequence] = '1'
             )
    SELECT  *
    FROM    GROUP1
    WHERE   [GROUP1].[SALES_ORDER_NUM] = @SALES_ORDER_NUMBER
            AND [GROUP1].[LINE_NUM] = @SALES_ORDER_LINE
    ORDER BY [GROUP1].[SID]
           ,[GROUP1].[OptionSequence]
           ,[GROUP1].[ComponentAttributeName]

	</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
