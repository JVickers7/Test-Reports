  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>LCINAVREP</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CUST">
            <Value>=Parameters!CUST.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOC">
            <Value>=Parameters!LOC.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
/*Configurator Option 1/31/12 KC*/
SELECT  PRICE.[Sales Code]
       ,SKU.[Item No_]
       ,SKU.[Variant Code]
       ,IV.Description
       ,CASE WHEN XREF.[Cross-Reference No_] = ''
             THEN ALTXREF.[Cross-Reference No_]
             ELSE XREF.[Cross-Reference No_]
        END AS CROSSREF
       ,BDSO.Description AS Attribute
       --,BDSO.ValueDescription AS VALUE
       ,BCA.Value
FROM    [LCI$Stockkeeping Unit] SKU
        INNER JOIN LCI$Item I ON SKU.[Item No_] = I.No_
        INNER JOIN [LCI$Item Variant] IV ON SKU.[Item No_] = IV.[Item No_]
                                            AND SKU.[Variant Code] = IV.Code
        INNER JOIN udv_SalesQuoteView PRICE ON SKU.[Item No_] = PRICE.[Item No_]
                                            AND SKU.[Variant Code] = PRICE.[Variant Code]
        --LEFT OUTER JOIN LCI$AVA_Head                                     
        LEFT OUTER JOIN [LCI$Item Cross Reference] AS XREF ON PRICE.[Variant Code] = XREF.[Variant Code]
                                            AND PRICE.[Item No_] = XREF.[Item No_]
                                            AND PRICE.[Sales Code] = XREF.[Cross-Reference Type No_]
                                            AND PRICE.[Unit of Measure Code] = XREF.[Unit of Measure]
        LEFT OUTER JOIN LCI$Customer CUST ON PRICE.[Sales Code] = CUST.No_
        LEFT OUTER JOIN [LCI$Item Cross Reference] AS ALTXREF ON PRICE.[Variant Code] = ALTXREF.[Variant Code]
                                            AND PRICE.[Item No_] = ALTXREF.[Item No_]
                                            AND CUST.[Bill-to Customer No_] = ALTXREF.[Cross-Reference Type No_]
                                            AND PRICE.[Unit of Measure Code] = XREF.[Unit of Measure]
        INNER JOIN BDCConfigurations BC ON BC.ConfigurationID = SKU.[Variant Code]COLLATE DATABASE_DEFAULT
                                           AND BC.HeaderID = 'VARIANT'
        INNER JOIN [BDCPages] BDCP ON BC.CfgID = BDCP.CfgID
        INNER JOIN BDCScreens BDS ON BDCP.[PageID] = BDS.[PageID]
        INNER JOIN BDCScreenOptions BDSO ON BDS.[ScreenID] = BDSO.[ScreenID]
                                            AND BDSO.Visible = 1
        INNER JOIN BDCComponents BC2 ON BC2.CfgID = BC.CfgID
        INNER JOIN BDCComponentAttributes BCA ON BC2.ComponentID = BCA.ComponentID
                                            AND BCA.ComponentAttributeID = BDSO.ComponentAttributeID
WHERE   SKU.[Variant Code] &lt;&gt; ''
       AND ( PRICE.[Sales Code] = @CUST )
        AND SKU.[Location Code] = @LOC
        AND IV.Blocked = 0
        
ORDER BY SKU.[Variant Code]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
